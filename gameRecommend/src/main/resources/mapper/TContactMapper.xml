<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gameRecommend.mapper.ContactMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.gameRecommend.entity.Contact">
        <id column="id" property="id" />
        <result column="email" property="email" />
        <result column="pay_by_wechat" property="payByWechat" />
        <result column="pay_by_zhi" property="payByZhi" />
        <result column="qq" property="qq" />
        <result column="wechat" property="wechat" />
        <result column="wechat_code" property="wechatCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, email, pay_by_wechat, pay_by_zhi, qq, wechat, wechat_code
    </sql>

</mapper>
